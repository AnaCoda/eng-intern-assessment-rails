<h1>Articles</h1>
<ul>
  <% @articles.each do |article| %>
    <li>
      <h1><%= article.title %><h3>
      <h2><%= article.author %><h2>
      <h3><%= article.date %><h3>
      <h4><%= article.content %><h4>
      <h5><%= link_to "Edit", edit_article_path(article) %><h5>
    </li>
  <% end %>
</ul>

<%= link_to "New Article", new_article_path %>